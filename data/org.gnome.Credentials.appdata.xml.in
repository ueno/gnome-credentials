<?xml version="1.0" encoding="UTF-8"?>
<component type="desktop">
  <id>org.gnome.Credentials.desktop</id>
  <name>GNOME Credentials</name>
  <summary>Password and key manager</summary>
  <licence>CC0</licence>
  <description>
    <_p>
      Credentials is a simple utility application to manage your passwords and keys.
    </_p>
    <_p>
      You can also import remote keys and publish your keys to keyserver.
    </_p>
  </description>
  <screenshots>
    <screenshot type="default" width="1700" height="1980">https://raw.githubusercontent.com/gnome-design-team/gnome-mockups/master/passwords-and-keys/passwords-and-keys.png</screenshot>
  </screenshots>
  <url type="homepage">https://wiki.gnome.org/Design/Apps/Credentials</url>
  <updatecontact>dueno_at_src.gnome.org</updatecontact>
</component>
